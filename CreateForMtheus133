public static void main(String[] args) {
                    Scanner in = new Scanner(System.in);
		        
		        String frase = umaFrase();
		        int caracteresLinha = 0;
		        int limitadorDeCaracteres =80;   
		        String [] resultado = new String [9];
		        
		         for(int i =0; i< resultado.length; i++) resultado[i]="";
		         
		         for(int indice = 0; indice < frase.length(); indice++){
		             char caractere = frase.charAt(indice);
		             
                             if(caractere=='l') caracteresLinha += funcaoL(resultado, true);
                             if(caractere=='m') caracteresLinha += funcaoM(resultado, true);
                             if(caractere=='a') caracteresLinha += funcaoA(resultado, true);            
                             if(caractere=='t') caracteresLinha += funcaoT(resultado, true);            
                             if(caractere=='h') caracteresLinha += funcaoH(resultado, true);           
                             if(caractere=='e') caracteresLinha += funcaoE(resultado, true);            
                             if(caractere=='u') caracteresLinha += funcaoU(resultado, true);           
                             if(caractere=='s') caracteresLinha += funcaoS(resultado, true);          
                             if(caractere=='g') caracteresLinha += funcaoG(resultado, true);          
                             if(caractere=='i') caracteresLinha += funcaoI(resultado, true);                 
                             if(caractere=='p') caracteresLinha += funcaoP(resultado, true);           
                             if(caractere=='r') caracteresLinha += funcaoR(resultado, true);        
                             if(caractere=='o') caracteresLinha += funcaoO(resultado, true);          
                             if(caractere=='c') caracteresLinha += funcaoC(resultado, true);
                             if(caractere=='d') caracteresLinha += funcaoD(resultado, true);
                             if(caractere=='v') caracteresLinha += funcaoV(resultado, true);
                             if(caractere=='n') caracteresLinha += funcaoN(resultado, true);
                             if(caractere=='y') caracteresLinha += funcaoY(resultado, true);
                             if(caractere=='f') caracteresLinha += funcaoF(resultado, true);
                             if(caractere=='b') caracteresLinha += funcaoB(resultado, true);
                             if(caractere=='k') caracteresLinha += funcaoK(resultado, true);
                             if(caractere=='j') caracteresLinha += funcaoJ(resultado, true);
                             if(caractere=='x') caracteresLinha += funcaoX(resultado, true);
                             if(caractere=='¨') caracteresLinha += funcaos2(resultado, true);
		             if(caractere==' ') caracteresLinha += funcaoEspaco(resultado, true);

                             if(caracteresLinha >= limitadorDeCaracteres){
		                    
                                 for(int i = 0; i < resultado.length; i++) System.out.println(resultado[i]);
		                 for(int i = 0; i < resultado.length; i++) resultado[i] = "";
		                 caracteresLinha=0;
		             
                                }
                             if(caractere=='l') caracteresLinha += funcaoL(resultado, false);
                             if(caractere=='m') caracteresLinha += funcaoM(resultado, false);
                             if(caractere=='a') caracteresLinha += funcaoA(resultado, false);            
                             if(caractere=='t') caracteresLinha += funcaoT(resultado, false);            
                             if(caractere=='h') caracteresLinha += funcaoH(resultado, false);           
                             if(caractere=='e') caracteresLinha += funcaoE(resultado, false);            
                             if(caractere=='u') caracteresLinha += funcaoU(resultado, false);           
                             if(caractere=='s') caracteresLinha += funcaoS(resultado, false);          
                             if(caractere=='g') caracteresLinha += funcaoG(resultado, false);          
                             if(caractere=='i') caracteresLinha += funcaoI(resultado, false);                 
                             if(caractere=='p') caracteresLinha += funcaoP(resultado, false);           
                             if(caractere=='r') caracteresLinha += funcaoR(resultado, false);        
                             if(caractere=='o') caracteresLinha += funcaoO(resultado, false);          
                             if(caractere=='c') caracteresLinha += funcaoC(resultado, false);
                             if(caractere=='d') caracteresLinha += funcaoD(resultado, false);
                             if(caractere=='v') caracteresLinha += funcaoV(resultado, false);
                             if(caractere=='n') caracteresLinha += funcaoN(resultado, false);
                             if(caractere=='y') caracteresLinha += funcaoY(resultado, false);
                             if(caractere=='f') caracteresLinha += funcaoF(resultado, false);
                             if(caractere=='b') caracteresLinha += funcaoB(resultado, false);
                             if(caractere=='k') caracteresLinha += funcaoK(resultado, false);
                             if(caractere=='j') caracteresLinha += funcaoJ(resultado, false);
                             if(caractere=='x') caracteresLinha += funcaoX(resultado, false);
                             if(caractere=='¨') caracteresLinha += funcaos2(resultado, false);
		             if(caractere==' ') caracteresLinha += funcaoEspaco(resultado, false);
                            
		        }
		        for(int i=0; i<resultado.length; i++) System.out.println(resultado[i]);    
		        
		}

		 public static String umaFrase(){
		        Scanner in = new Scanner (System.in);
		        System.out.println("digite seu nome");
		        String frase = in.nextLine();
		        return frase;
		    }
		 
                 
                 
                 
                 
                public static int funcaos2(String[] vet, boolean aparecerSoTamanho){
			if(aparecerSoTamanho){
                            return ("" + " " + "OO          OO").length();
                        }
                        vet [0] = vet[0] + "  "+"   OOO   OOO  ";
                        vet [1] = vet[1] + "  "+"  OOOOO OOOOO ";
                        vet [2] = vet[2] + "  "+"  OOOOOOOOOOO ";
                        vet [3] = vet[3] + "  "+"   OOOOOOOOO  ";
                        vet [4] = vet[4] + "  "+"    OOOOOOO   ";
                        vet [5] = vet[5] + "  "+"     OOOOO    ";
                        vet [6] = vet[6] + "  "+"       O      ";  
                return 0;        
                } 
		public static int funcaoM(String[] vet, boolean aparecerSoTamanho){
			if(aparecerSoTamanho){
                            return ("" + " " + "OO          OO").length();
                        }
                        vet [0] = vet[0] + "  "+"OO          OO";
                        vet [1] = vet[1] + "  "+"OOO        OOO";
                        vet [2] = vet[2] + "  "+"OO OO    OO OO";
                        vet [3] = vet[3] + "  "+"OO  OOOOOO  OO";
                        vet [4] = vet[4] + "  "+"OO   OOOO   OO";
                        vet [5] = vet[5] + "  "+"OO    OO    OO";
                        vet [6] = vet[6] + "  "+"OO    OO    OO";
                return 0;        
		}
