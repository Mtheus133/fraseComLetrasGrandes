		public static int funcaoEspaco(String[] vet, boolean aparecerSoTamanho){
			if(aparecerSoTamanho){
                            return("              ").length();
                        }
                        for(int contador = 0; contador < vet.length; contador++) {
				vet[contador] += "             ";
			}  
                 return 0;
                }
}
